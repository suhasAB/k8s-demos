CREATE DATABASE accenture;
USE accenture;
CREATE TABLE employees (id INT AUTO_INCREMENT PRIMARY KEY, first_name VARCHAR(50)
NOT NULL, last_name VARCHAR(50) NOT NULL);
INSERT INTO employees (first_name, last_name) VALUES ('AB', 'Deviliers');
INSERT INTO employees (first_name, last_name) VALUES ('Virat', 'Kohli'); 
INSERT INTO employees (first_name, last_name) VALUES ('Chris', 'Gayle'); 
SELECT * FROM employees;
